{
    "name": "NethServer Phone Home",
    "description": "Display NethServer Phone Home installation number",
    "version": "0.1",
    "author": "Alessandro Polidori",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "lib/jquery-2.1.3.min.js",
            "js/background.js"
        ]
    },
    "permissions": [
        "http://www.nethserver.org/"
    ],
    "icons": {
        "16":  "img/logo16.png",
        "32":  "img/logo32.png",
        "48":  "img/logo48.png",
        "128": "img/logo128.png",
        "256": "img/logo128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/logo19.png",
            "38": "img/logo38.png"
        }
    }
}